    "import numpy as np\n",
    "from sklearn.linear_model import LinearRegression\n",
    "x = np.array([[1], [2], [4], [0]])\n",
    "y = np.array([0.5,1, 2, 0])\n",
    "\n",
    "model = LinearRegression(fit_intercept=True)\n",
    "\n",
    "model.fit(x, y)\n",
    "print(model.intercept_)\n",
    "print(model.coef_)"
   ]
  }
